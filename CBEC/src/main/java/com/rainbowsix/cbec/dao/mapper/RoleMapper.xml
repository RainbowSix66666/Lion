<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.rainbowsix.cbec.dao.IRoleDao">

	<insert id="create" parameterType="com.rainbowsix.cbec.model.RoleModel">
		insert into user_role (roleName, detial) values (#{name}, #{detial})
	</insert>
	
	<update id="update" parameterType="com.rainbowsix.cbec.model.RoleModel"></update>
	<update id="setMenuId" parameterType="com.rainbowsix.cbec.model.RoleModel"></update>
	<delete id="delete" parameterType="com.rainbowsix.cbec.model.RoleModel"></delete>
	<select id="selectByRoleName" parameterType="string"></select>
	<select id="selectAll"></select>
	
</mapper>